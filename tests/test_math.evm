
global ghundret = 100;

function fibonacci(n);

// this is probaly the stupidiest way to calculate a
// fibonacci number - but it is a nice recursion test..
function fibonacci(n)
{
	if (n < 2)
		return n;
	return fibonacci(n-1) + fibonacci(n-2);
		
}

function main()
{
	local one, two, thousand, negthousand;

	one = 1;
	two = one;
	two++;

	thousand = two * 500;
	negthousand = -thousand;

	$uf1(1 + 1 * 2);
	$uf1(one + one * two);
	$uf1(thousand / ghundret);
	$uf1(0x7fff / negthousand);

	$uf1(-0x7fff / thousand);
	$uf1(-0x7fff / negthousand);

	$uf1(fibonacci(6));
}

